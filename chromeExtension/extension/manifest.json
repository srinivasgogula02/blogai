{
    "manifest_version": 3,
    "name": "Browsing Activity Tracker",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "activeTab",
      "clipboardRead",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["http://127.0.0.1/*", "http://your-backend-url/*"]
  }
  